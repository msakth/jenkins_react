pipeline {
    environment {
        CI = 'true' 
    }
    stages {
        stage('Test') {
            steps {
                nvm install v12.13.1
                sh 'node --version'
            }
        }
        stage('Build') {
            steps {
                sh 'npm install'
            }
        }
        stage('UnitTest') { 
            steps {
                sh './jenkins/scripts/test.sh' 
            }
        }
    }
}